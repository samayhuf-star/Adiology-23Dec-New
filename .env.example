# LambdaTest Credentials
# Get your credentials from: https://automation.lambdatest.com/user-settings/api-token
VITE_LAMBDATEST_USERNAME=your-username-here
VITE_LAMBDATEST_ACCESS_KEY=your-access-key-here

# LambdaTest Playwright Configuration (for Playwright tests)
# These are used by playwright.lambdatest.config.ts
LT_USERNAME=your-username-here
LT_ACCESS_KEY=your-access-key-here

# Optional: Application URL for testing
VITE_BASE_URL=http://localhost:3000
BASE_URL=http://localhost:3000

# Optional: Build and Project names for LambdaTest
BUILD_NAME=
PROJECT_NAME=Adiology Campaign Dashboard

# Optional: LambdaTest Tunnel Configuration
LT_TUNNEL=false
LT_TUNNEL_NAME=

# AWS Configuration for VM Management and Email Service
# Get these from AWS IAM Console
AWS_ACCESS_KEY_ID=your-aws-access-key-id
AWS_SECRET_ACCESS_KEY=your-aws-secret-access-key
AWS_REGION=us-east-1
AWS_DEFAULT_REGION=us-east-1

# VM Configuration
VM_PROVIDER=aws
VM_DEFAULT_REGION=us-east-1
VM_PRICING_MARKUP=0.20

# Stripe Configuration for VM Billing
STRIPE_SECRET_KEY=sk_test_your-stripe-secret-key
STRIPE_PUBLISHABLE_KEY=pk_test_your-stripe-publishable-key
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your-stripe-publishable-key

# Email API Configuration
VITE_EMAIL_API_URL=http://localhost:5001

# Supabase Configuration (if using)
VITE_SUPABASE_URL=your-supabase-url
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key
